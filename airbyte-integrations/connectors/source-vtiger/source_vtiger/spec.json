{
  "documentationUrl": "https://docsurl.com",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Vtiger Spec",
    "type": "object",
    "required": ["host", "username", "accessKey"],
    "additionalProperties": false,
    "properties": {
      "host": {
        "title": "Host",
        "description": "Host of your vtiger instance",
        "type": "string",
        "order": 0
      },
      "username": {
        "title": "Username",
        "description": "Username to use to access vtiger.",
        "type": "string",
        "order": 1
      },
      "accessKey": {
        "title": "Access Key",
        "description": "Access Key associated with the Username.",
        "type": "string",
        "airbyte_secret": true,
        "order": 2
      }
    }
  }
}
